ENV=DIR=LOCAL=hd0\A\sys\api\commands\%UFE_VER%\x86\Choice
ENV=DIR=LOCAL=hd0\A\sys\api\commands\%UFE_VER%\x86\inifile
ENV=DIR=LOCAL=hd0\A\sys\api\commands\%UFE_VER%\x86\Timeout
