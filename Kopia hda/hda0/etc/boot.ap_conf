[INSTALLATION]
INSTALL_DRIVE=hd0
INSTALL_PAR=A

[WINDOW_CONFIG]
SIZE=75=45
CODE_PAGE=048,852
TITLE=Agent PenDrive v1.6

[MAIN_CONFIG]
BOOT=hd0\A\boot\fusion\boot.bat
UFE_VER=UFE_2.1.0169
CONSOLE=hd0\A\boot\%UFE_VER%\console.bat
KERNEL=hd0\A\boot\%UFE_VER%\krnl16.bat
KERNEL_LOADER=hd0\A\boot\%UFE_VER%\kernel_loader.bat
SYSTEM=hd0\A\boot\system\system.ap_app -system
SYSTEM_LOADER=hd0\A\boot\system\system.ap_app -system_loader

[VARIABLES]
VAR=LOCAL=AP_DIR=hd0\A
VAR=LOCAL=AP_TMP=hd0\A\tmp
VAR=LOCAL=API=hd0\A\sys\api
VAR=LOCAL=APPS=hd0\A\usr\bin\apps
VAR=LOCAL=BIN=hd0\A\bin
VAR=LOCAL=BOOT_DIR=hd0\A\boot
VAR=LOCAL=COMS=hd0\A\bin\coms
VAR=LOCAL=COMMANDS=hd0\A\sys\api\commands
VAR=LOCAL=ETC=hd0\A\etc
VAR=LOCAL=ENV=hd0\A\sys\env
VAR=LOCAL=FTP=hd0\A\var\ftp
VAR=LOCAL=FUSION=hd0\A\boot\fusion
VAR=LOCAL=KERNEL_DIR=hd0\A\boot\kernel
VAR=LOCAL=LIB=hd0\A\lib
VAR=LOCAL=MODS=hd0\A\bin\mods
VAR=LOCAL=SYSTEM_DIR=hd0\A\boot\system
VAR=LOCAL=RAM=RAM
VAR=LOCAL=REG_DIR=hd0\A\etc\reg
VAR=LOCAL=VAR=hd0\A\var
VAR=LOCAL=VARS=hd0\A\sys\var

[ENVIRONMENT]
ENV=DIR=LOCAL=hd0\A\sys\api
ENV=DIR=LOCAL=hd0\A\sys\api\classes
ENV=DIR=LOCAL=hd0\A\sys\api\procedures
