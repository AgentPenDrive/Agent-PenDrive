[CONFIG]
DEV_TYPE=010000110101000001010101

[DEV_CONFIG]
DEV_FREQUENCY={INHERITED}
DEV_ARCHITECTURE={INHERITED}

[DEV_INSTRUCTION_SET]
DEV_INSTRUCTION="AND"="devices\cpu\#instructions\$and.bat"
DEV_INSTRUCTION="NAND"="devices\cpu\#instructions\$nand.bat"
DEV_INSTRUCTION="NOR"="devices\cpu\#instructions\$nor.bat"
DEV_INSTRUCTION="NOT"="devices\cpu\#instructions\$not.bat"
DEV_INSTRUCTION="OR"="devices\cpu\#instructions\$or.bat"
DEV_INSTRUCTION="XOR"="devices\cpu\#instructions\$xor.bat"
DEV_INSTRUCTION="XNOR"="devices\cpu\#instructions\$xnor.bat"
DEV_INSTRUCTION="TS"="devices\cpu\#instructions\$ts.bat"

[DEV_STORAGE]
