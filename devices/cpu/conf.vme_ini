DEV_STORAGE_ADRESS="media\cpu\cache\l1"
DEV_STORAGE_SPACE=65536
DEV_STORAGE2=1
DEV_STORAGE2_ADRESS="media\cpu\cache\l2"
DEV_STORAGE2_SPACE=524288