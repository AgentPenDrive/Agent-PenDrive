[MAIN_CONFIG]
UFE_DIR=boot
UFE_VER=UFE_2.1.0169
CONSOLE=console.bat
KERNEL=krnl16.bat
KERNEL_LOADER=kernel_loader.bat
SYSTEM=sys\system\sys16.ap_app -system
SYSTEM_LOADER=sys\system\sys16.ap_app -system_loader

[CONSTANTS]
CONST=LOCAL=AP_TMP=tmp
CONST=LOCAL=API=sys\api
CONST=LOCAL=APPS=usr\bin\apps
CONST=LOCAL=BIN=bin
CONST=LOCAL=CMDS=sys\api\cmds
CONST=LOCAL=COMS=bin\coms
CONST=LOCAL=CONSTS=var\const
CONST=LOCAL=ETC=etc
CONST=LOCAL=ENV=sys\env
CONST=LOCAL=FTP=var\ftp
CONST=LOCAL=FUSION=boot\fusion
CONST=LOCAL=LIB=lib
CONST=LOCAL=MODS=lib\modules
CONST=LOCAL=SYS=sys
CONST=LOCAL=SYSTEM_DIR=sys\system
CONST=LOCAL=REG_DIR=etc\modules\reg
CONST=LOCAL=VAR=var

[ENVIRONMENT]
ENV=DIR=LOCAL=sys\api
ENV=DIR=LOCAL=sys\api\classes
ENV=DIR=LOCAL=sys\api\procedures
ENV=DIR=LOCAL=sys\api\functions
ENV=DIR=LOCAL=sys\api\labels
ENV=DIR=LOCAL=bin\cmds\Choice
ENV=DIR=LOCAL=bin\cmds\inifile
ENV=DIR=LOCAL=bin\cmds\Timeout
ENV=DIR=LOCAL=bin\cmds\Tscmd
ENV=DIR=LOCAL=bin\cmds\wbat250
ENV=DIR=LOCAL=bin\cmds\WinRAR

[LIBRARIES]
LIB=SYS=LOCAL=lib\sys\hal.ap_sys

[MODULES]
MOD=var
MOD=env
MOD=lib
MOD=run
MOD=reg