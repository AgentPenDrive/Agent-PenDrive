[MAIN_CONFIG]
UFE_DIR=boot
UFE_VER=UFE_2.1.0169
CONSOLE=console.bat
KERNEL=krnl16.bat
KERNEL_LOADER=kernel_loader.bat
KERNEL_LOAD_TYPE=2
SYSTEM=sys\system\sys16.ap_app -system
SYSTEM_LOADER=sys\system\sys16.ap_app -system_loader

[VARIABLES]
VAR=LOCAL=AP_TMP=tmp
VAR=LOCAL=API=sys\api
VAR=LOCAL=APPS=usr\bin\apps
VAR=LOCAL=BIN=bin
VAR=LOCAL=COMS=bin\coms
VAR=LOCAL=CMDS=sys\api\cmds
VAR=LOCAL=ETC=etc
VAR=LOCAL=ENV=sys\env
VAR=LOCAL=FTP=var\ftp
VAR=LOCAL=FUSION=boot\fusion
VAR=LOCAL=LIB=lib
VAR=LOCAL=MODS=lib\modules
VAR=LOCAL=SYS=sys
VAR=LOCAL=SYSTEM_DIR=sys\system
VAR=LOCAL=REG_DIR=etc\modules\reg
VAR=LOCAL=VAR=var
VAR=LOCAL=CONST=var\const

[ENVIRONMENT]
ENV=DIR=LOCAL=sys\api
ENV=DIR=LOCAL=sys\api\classes
ENV=DIR=LOCAL=sys\api\procedures
ENV=DIR=LOCAL=sys\api\functions
ENV=DIR=LOCAL=sys\api\labels
ENV=DIR=LOCAL=sys\api\cmds\%UFE_VER%\x86\Choice
ENV=DIR=LOCAL=sys\api\cmds\%UFE_VER%\x86\inifile
ENV=DIR=LOCAL=sys\api\cmds\%UFE_VER%\x86\Timeout
ENV=DIR=LOCAL=sys\api\cmds\%UFE_VER%\x86\Tscmd
ENV=DIR=LOCAL=sys\api\cmds\%UFE_VER%\x86\wbat250
ENV=DIR=LOCAL=sys\api\cmds\%UFE_VER%\x86\WinRAR
ENV=DIR=LOCAL=sys\bin\bash

[LIBRARIES]
LIB=SYS=LOCAL=lib\sys\hal.ap_sys